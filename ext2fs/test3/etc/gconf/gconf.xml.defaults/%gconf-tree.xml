<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="nm-applet">
				<entry name="disable-wifi-create" mtime="1339951410" type="schema" stype="bool" owner="nm-applet" gettext_domain="nm-applet">
					<local_schema locale="C" short_desc="Disable WiFi Create">
						<default type="bool" value="false"/>
						<longdesc>
          Set to TRUE to disable creation of adhoc networks when using the applet.
        </longdesc>
					</local_schema>
				</entry>
				<entry name="stamp" mtime="1339951410" type="schema" stype="int" owner="nm-applet" gettext_domain="nm-applet">
					<local_schema locale="C" short_desc="Stamp">
						<default type="int" value="0"/>
						<longdesc>
          Used to determine whether settings should be migrated to a
          new version.
        </longdesc>
					</local_schema>
				</entry>
				<entry name="suppress-wireless-networks-available" mtime="1339951410" type="schema" stype="bool" owner="nm-applet" gettext_domain="nm-applet">
					<local_schema locale="C" short_desc="Suppress networks available notifications">
						<default type="bool" value="false"/>
						<longdesc>
          Set this to TRUE to disable notifications when wireless
          networks are available.
        </longdesc>
					</local_schema>
				</entry>
				<entry name="disable-disconnected-notifications" mtime="1339951410" type="schema" stype="bool" owner="nm-applet" gettext_domain="nm-applet">
					<local_schema locale="C" short_desc="Disable disconnected notifications">
						<default type="bool" value="false"/>
						<longdesc>
          Set this to TRUE to disable notifications when disconnecting
          from a network.
        </longdesc>
					</local_schema>
				</entry>
				<entry name="disable-connected-notifications" mtime="1339951410" type="schema" stype="bool" owner="nm-applet" gettext_domain="nm-applet">
					<local_schema locale="C" short_desc="Disable connected notifications">
						<default type="bool" value="false"/>
						<longdesc>
          Set this to TRUE to disable notifications when connecting to
          a network.
        </longdesc>
					</local_schema>
				</entry>
			</dir>
		</dir>
	</dir>
	<dir name="apps">
		<dir name="nm-applet">
			<entry name="disable-wifi-create" mtime="1339951410" schema="/schemas/apps/nm-applet/disable-wifi-create"/>
			<entry name="stamp" mtime="1339951410" schema="/schemas/apps/nm-applet/stamp"/>
			<entry name="suppress-wireless-networks-available" mtime="1339951410" schema="/schemas/apps/nm-applet/suppress-wireless-networks-available"/>
			<entry name="disable-disconnected-notifications" mtime="1339951410" schema="/schemas/apps/nm-applet/disable-disconnected-notifications"/>
			<entry name="disable-connected-notifications" mtime="1339951410" schema="/schemas/apps/nm-applet/disable-connected-notifications"/>
		</dir>
	</dir>
</gconf>
