prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
system_bus_default_address=unix:path=/usr/var/run/dbus/system_bus_socket
sysconfdir=/usr/etc
session_bus_services_dir=/usr/share/dbus-1/services
system_bus_services_dir=/usr/share/dbus-1/system-services
interfaces_dir=/usr/share/dbus-1/interfaces
daemondir=/usr/bin

Name: dbus
Description: Free desktop message bus
Version: 1.7.4
Libs: -L${libdir} -ldbus-1
Libs.private: -lpthread   
Cflags: -I${includedir}/dbus-1.0 -I${libdir}/dbus-1.0/include 
